{
  "presets": [
    ["env", { "modules": false }],
    "react",
    "es2015",
    "stage-2"
  ],
  "plugins": ["transform-runtime", "transform-decorators-legacy"],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  },
  "ignore": [
    "build",
    "node_modules",
    "static",
    "test"
  ]
}
